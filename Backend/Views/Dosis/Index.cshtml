@model IEnumerable<ModelCL.Dosis>

@{
    ViewBag.Title = "Dosis";
}

<p>
    @Html.ActionLink("Crear", "Create")
</p>
<div class="row">
    <div class="col-md-12">
        <div class="x_panel">

            <table class="table">
                <tr>
                    <th>
                        Nombre Medicina
                    </th>
                    <th>
                        Valor
                    </th>
                    <th>
                       Presentación
                    </th>
                    <th>
                        Cantidad Minima de Dosis
                    </th>
                    <th>
                        Efecto de la Dosis
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.RelMedVal.Medicina.MedicinaNombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.RelMedVal.Valor.ValorNombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Presentacion.PresentacionNombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DosisCantidadMin)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DosisEfecto)
                        </td>
                        <td>
                            @Html.ActionLink("Editar", "Edit", new { id = item.DosisId }) |
                            @Html.ActionLink("Eliminar", "Delete", new { id = item.DosisId })
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>
