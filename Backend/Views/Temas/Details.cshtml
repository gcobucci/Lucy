@model ModelCL.Tema

@{
    ViewBag.Title = "Ver Tema";
}


<div>
    <h4>Tema</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.Label("Nombre")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TemaNombre)
        </dd>

        <dt>
           @Html.Label("Descripción")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TemaDescripcion)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.TemaId }) |
    @Html.ActionLink("Volver al listado", "Index")
</p>
